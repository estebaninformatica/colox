<% model = travel_operator %>
<%= form_for(model, remote: true) do |f| %>
<tr>
    <td>
        <div class="field">
            <%= f.text_field :name,:class => "name" %>
        </div>
    </td>
    <td>
        <div class="field">
            <%= f.hidden_field :operator_aptour_id, {:id => "model"+model.id.to_s,:class => "aptour_select2 " }%>
        </div>
    </td>

    <td >
        <div class="actions">
        <%= f.submit "Guardar", class: "btn btn-primary btn-sm" %>
        </div>
    </td>
</tr>
<% end %>
