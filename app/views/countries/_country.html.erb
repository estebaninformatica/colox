<%= form_for(country, remote: true) do |f| %>
<tr>
    <td>
	    <div class="field">
	   		<%= f.text_field :name %>
	  	</div>
  	</td>
    <td>
		<div class="field">
			<%= f.hidden_field :destination_aptour_id, {:class => "aptour_destinations_select2 span12" }%>
